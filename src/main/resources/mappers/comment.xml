<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gma.gmagame.mapper.CommentMapper">

    <select id="selectCommentList" resultType="com.gma.gmagame.model.Comment">
        SELECT *
        FROM comment_aa
        WHERE bno=#{bno}
    </select>

</mapper>